<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
	<appSettings>
		<!-- Cấu hình PLC -->
		<add key="PLC_IP" value="192.168.1.100" />
		<add key="PLC_Port" value="502" />
		<!-- ví dụ Modbus TCP -->

		<!-- Cấu hình COM -->
		<add key="COM_PortName" value="COM3" />
		<add key="COM_BaudRate" value="9600" />
		<add key="COM_Parity" value="None" />
		<add key="COM_DataBits" value="8" />
		<add key="COM_StopBits" value="One" />
	</appSettings>
</configuration>